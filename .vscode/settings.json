{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.format.enable": true,
	"editor.codeActionsOnSave": [
		"source.formatDocument",
		"source.fixAll.eslint"
	],
	"editor.tabSize": 2,
	"eslint.validate": [
		"javascript",
		"typescript",
		"typescriptreact",
		"json",
		"jsonc",
		"json5"
	],
}